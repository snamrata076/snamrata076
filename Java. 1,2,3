package jpr;
public class javaproject4
 {
static int s;
static double a ;
static char d;
static float f;
int per=96;
public static void main(String[] args) {
System.out.println(s);
System.out.println(a);
System.out.println(d); 
System.out.println(f);
System.out.println(per); //non-static variable per cannot be referenced from a static context
}

}
